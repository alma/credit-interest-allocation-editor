{
  "name": "alma-credit-interest-allocation-editor",
  "version": "1.0.0",
  "description": "Alma Credit Interest Allocation Editor",
  "scripts": {
    "analyse": "elm-analyse",
    "build": "npm run cleanup && npm run copy-assets && elm make src/CreditInterestAllocationEditor.elm --output=build/app.js --optimize && uglifyjs build/app.js --compress 'pure_funcs=\"F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9\",pure_getters,keep_fargs=false,unsafe_comps,unsafe' | uglifyjs --mangle --output build/app.min.js",
    "copy-assets": "rimraf build && mkdir build && cp -r public/* build",
    "cleanup": "rimraf elm-stuff/",
    "debug": "elm-live src/CreditInterestAllocationEditor.elm --port=3000 --dir=public/ -- --output=public/app.js --debug",
    "deploy": "npm run build && gh-pages --dist build/",
    "live": "elm-live src/CreditInterestAllocationEditor.elm --port=3000 --dir=public/ -- --output=public/app.js",
    "test": "elm-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alma/credit-interest-allocation-editor.git"
  },
  "bugs": {
    "url": "https://github.com/alma/credit-interest-allocation-editor/issues"
  },
  "homepage": "https://github.com/alma/credit-interest-allocation-editor#readme",
  "author": "Alma",
  "license": "ISC",
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-analyse": "^0.16.5",
    "elm-format": "^0.8.3",
    "elm-live": "^4.0.2",
    "elm-test": "^0.19.1-revision2",
    "gh-pages": "^2.1.1",
    "rimraf": "^3.0.0",
    "uglify-js": "^3.12.1"
  }
}
